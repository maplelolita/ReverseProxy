{
  "Kestrel": {
    "Certificates": {
      "Default": {
        "Path": "/https/full.pem",
        "KeyPath": "/https/key.pem"
      }
    }
  },
  "URLS": "https://+;http://+",
  "HTTP_PORT": "80",
  "HTTPS_PORT": "443",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ForwardedHeadersOptions": {
    "ForwardedHeaders": "All",
    "AllowedHosts": [ "*" ]
  },
  "apollo": {
    "AppId": "appid",
    "Env": "DEV",
    "MetaServer": "http://metaserver/",
    "ConfigServer": [ "http://configserver/" ],
    "Namespaces": [ "application" ]
  }
}